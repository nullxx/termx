{
  "name": "termx",
  "productName": "termx",
  "version": "1.0.0",
  "description": "A simple SSH terminal app.",
  "main": "dist/main.js",
  "homepage": "./",
  "repository": {
    "type": "git",
    "url": "https://github.com/nullxx/termx.git"
  },
  "scripts": {
    "start": "yarn run start:react \"wait-on http://localhost:3000 && yarn run transpile:electron && yarn run start:forge \"",
    "build": "react-scripts build",
    "start:forge": "electron-forge start",
    "start:react": "concurrently \"BROWSER=none react-scripts start\"",
    "package": "electron-forge package",
    "make": "yarn run transpile:electron && yarn run build && electron-forge make",
    "publish": "electron-forge publish",
    "transpile:electron": "tsc --p tsconfig-electron.json"
  },
  "keywords": [],
  "author": {
    "name": "Jon Lara",
    "email": "mail@nullx.me"
  },
  "license": "MIT",
  "config": {
    "forge": "./forge.config.js"
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.3.5",
    "electron-squirrel-startup": "^1.0.0",
    "keytar": "7.6.0",
    "ssh2": "^1.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-dmg": "^6.0.0-beta.57",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/publisher-github": "^6.0.0-beta.57",
    "@types/electron": "^1.6.10",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.4",
    "@types/react": "^17.0.11",
    "@types/react-alert": "^5.2.0",
    "@types/react-dom": "^17.0.8",
    "@types/react-router-dom": "^5.1.7",
    "@types/ssh2": "^0.5.46",
    "@types/styletron-engine-atomic": "^1.1.0",
    "@types/styletron-react": "^5.0.2",
    "@types/uuid": "^8.3.0",
    "atomize": "^1.0.26",
    "concurrently": "^6.2.0",
    "dotenv": "^10.0.0",
    "electron": "13.1.1",
    "eslint": "^7.28.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-alert": "^7.0.3",
    "react-animation": "^1.2.2",
    "react-contexify": "^5.0.0",
    "react-context-menu": "^3.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-transition-group": "^4.4.2",
    "styletron-engine-atomic": "^1.4.8",
    "styletron-react": "^6.0.1",
    "typescript": "^4.3.4",
    "wait-on": "^5.3.0",
    "xterm-addon-web-links": "^0.4.0",
    "xterm-for-react": "^1.0.4",
    "xterm-theme": "^1.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
